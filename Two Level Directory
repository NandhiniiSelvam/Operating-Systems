#include <stdio.h>

int main(){
    int users, files, i, j;
    char fname[10][10][20];

    printf("Enter number of users: ");
    scanf("%d",&users);

    for(i=0;i<users;i++){
        printf("User %d files count: ",i+1);
        scanf("%d",&files);

        for(j=0;j<files;j++){
            printf("Enter file: ");
            scanf("%s",fname[i][j]);
        }
    }

    printf("\nDirectory:\n");
    for(i=0;i<users;i++){
        printf("User %d:\n",i+1);
        for(j=0;j<10;j++){
            if(fname[i][j][0]!='\0')
                printf("%s\n",fname[i][j]);
        }
    }
    return 0;
}

